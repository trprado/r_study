1567395204470:# vari치veis dummy.
1567395204472:cd_dummy<-dummy.code(df_comp$cd)[,-1]
1567395204540:multi_dummy<-dummy.code(df_comp$multi)[,-1]
1567395204618:premium_dummy<-dummy.code(df_comp$premium)[,-1]
1567395204686:# cria칞칚o do novo dataframe com vari치veis dummys e remo칞칚o das vari치veis categoricas.
1567395204688:dados<-cbind(df_comp[,1:5],cd_dummy, multi_dummy, premium_dummy)
1567395204716:head(dados)
1567395204849:# Sele칞칚o de vari치veis pelo m칠todo forward
1567395204851:m0 <- lm(log(price) ~ speed, data = dados)
1567395204902:m1 <- update(m0, ~ . + ram)
1567395204963:m2 <- update(m1, ~ . + hd)
1567395205015:m3 <- update(m2, ~ . + screen)
1567395205074:m4 <- update(m3, ~ . + cd_dummy)
1567395205128:m5 <- update(m4, ~ . + multi_dummy)
1567395205209:m6 <- update(m5, ~ . + premium_dummy)
1567395205287:mtable(m0,m1,m2,m3,m4,m5,m6)
1567395205573:m7 <- update(m6, ~ . -multi_dummy)
1567395205647:mtable(m0,m1,m2,m3,m4,m5,m6, m7)
1567395205936:# verifica칞칚o do pressuposto de normalidade dos residuos.
1567395205938:# Hip칩tese nula H0: Os res칤duos tem distribui칞칚o normal.
1567395205940:# Hip칩tese alternativa Ha: Os res칤duos n칚o tem distribui칞칚o normal.
1567395205942:ks.test(rstudent(m7), pnorm, mean(rstudent(m7)), sd(rstudent(m7)))
1567395205964:# remo칞칚o de outliers.
1567395205966:outliers<-c(which(rstudent(m7)>=3), which(rstudent(m7)<=(-3)))
1567395205974:dados<-dados[-outliers,]
1567395205989:# remo칞칚o da vari치vel multi_dummy (n칚o significativa)
1567395205991:m8 <- update(m6, ~ . -multi_dummy, data = dados)
1567395206060:ks.test(rstudent(m8), pnorm, mean(rstudent(m8)), sd(rstudent(m8)))
1567395206081:mtable(m8)
1567395206129:# remo칞칚o da vari치vel cd_dummy (n칚o significativa)
1567395206131:m9 <- update(m8, ~ . -cd_dummy, data = dados)
1567395206186:mtable(m9)
1567395206223:ks.test(rstudent(m9), pnorm, mean(rstudent(m9)), sd(rstudent(m9)))
1567395206236:# plot(fitted.values(m9), residuals(m9))
1567395206239:# abline(h=0, col="red")
1567395206247:round(coefficients(m9), 4)
1567395206254:ggplot(data = dados,
1567395206256:aes(x=fitted.values(m9), y=residuals(m9))) +
1567395206265:geom_point() +
1567395206267:geom_hline(yintercept = 0, color = 'red') +
1567395206268:ggtitle('Figura 16. Gr치fico dos valores ajustados versus res칤duos')
1567395206826:fit.model<-m9
1567395206892:attach(dados)
1567395206909:source("https://www.ime.usp.br/~giapaula/envel_norm")
1567395292250:ggplot(data=dados,
1567395292252:aes(x=price, y=predict(m9))) +
1567395292254:geom_point() +
1567395292255:geom_smooth(method = "lm") +
1567395292257:ggtitle('Figura 18. Gr치fico dos valores observados versus valores preditos e equa칞칚o do \nmodelo')
